# Frontend Environment Variables
VITE_API_URL=http://localhost:3001/api/v1
VITE_WS_URL=ws://localhost:8080/ws
VITE_APP_NAME=SecureVault
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# Security Configuration
VITE_ENCRYPTION_ENABLED=true
VITE_MFA_ENABLED=true
VITE_BIOMETRIC_ENABLED=true
VITE_SESSION_TIMEOUT=3600000

# Feature Flags
VITE_ZERO_KNOWLEDGE=true
VITE_ADVANCED_CRYPTO=true
VITE_INTRUSION_DETECTION=true
VITE_BACKUP_SYSTEM=true
VITE_PERFORMANCE_MONITORING=true

# Localization
VITE_DEFAULT_LANGUAGE=vi
VITE_SUPPORTED_LANGUAGES=vi,en

# Development
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug