# Admin Dashboard Environment Variables
VITE_API_URL=http://localhost:8080/api/v1
VITE_WS_URL=ws://localhost:8080/ws
VITE_APP_NAME=SecureVault Admin
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# Admin Configuration
VITE_ADMIN_SESSION_TIMEOUT=7200000
VITE_ADMIN_MFA_REQUIRED=true
VITE_ADMIN_IP_WHITELIST=127.0.0.1,::1

# Security Features
VITE_IDS_ENABLED=true
VITE_BACKUP_MANAGEMENT=true
VITE_AUDIT_LOGGING=true
VITE_SECURITY_MONITORING=true
VITE_DR_MANAGEMENT=true

# Monitoring & Alerting
VITE_PERFORMANCE_MONITORING=true
VITE_REAL_TIME_ALERTS=true
VITE_METRICS_REFRESH_INTERVAL=30000

# Localization
VITE_DEFAULT_LANGUAGE=vi
VITE_SUPPORTED_LANGUAGES=vi,en

# Development
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug